@model List<Size>
@{
    ViewData["Title"] = "Index";
}

<div class="table-responsive pt-3">
                    <table class="table table-dark">
                      <thead>
                        <tr>
                          <th>
                            Id
                          </th>
                          <th>
                            Name
                          </th>
                          
                        </tr>
                      </thead>
                      <tbody>
                        @foreach (Size size in Model)
                       {
                           <tr>
                          <td>
                            @size.Id
                          </td>
                          <td>
                            @size.Name;
                          </td>
                        </tr>
                       }
                      </tbody>
                    </table>
                  </div>

